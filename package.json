{
  "name": "@catho/quantum-storybook-ui",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "dependencies": {
    "react-copy-to-clipboard": "^5.0.1",
    "react-highlight": "^0.12.0",
    "react-markdown": "^4.0.6",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.84.0"
  },
  "peerDependencies": {
    "react": ">= 16",
    "react-dom": ">= 16",
    "styled-components": ">= 4"
  },
  "scripts": {
    "build": "yarn build:lib && yarn build:dist",
    "build:dist": "rollup -c",
    "build:lib": "babel ./src --out-dir lib --copy-files --ignore __tests__,spec.js,test.js,__snapshots__",
    "prebuild": "rimraf dist",
    "prebuild:lib": "rimraf lib",
    "build-storybook": "build-storybook -s public",
    "storybook": "start-storybook -p 9009 -s public",
    "prepublish": "yarn build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^4.1.6",
    "@storybook/addon-links": "^4.1.6",
    "@storybook/addons": "^4.1.6",
    "@storybook/react": "^4.1.6",
    "babel-loader": "^8.0.5",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "rimraf": "^2.6.3",
    "rollup": "^1.1.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-css-only": "^0.4.0",
    "rollup-plugin-filesize": "^6.0.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-svg": "^1.0.1",
    "rollup-plugin-url": "^2.1.0",
    "rollup-plugin-visualizer": "^0.9.2",
    "styled-components": "^4.1.3"
  }
}
